{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "outputs": ["dist/**"],
            "dependsOn": ["^build"]
        },
        "start": {
            "dependsOn": ["^start"],
            "persistent": true,
            "cache": false
        }
    }
}
